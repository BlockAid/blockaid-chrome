{
  "name": "BlockAid",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A extension to speed up page load",
  "homepage_url": "https://github.com/codeweft/blockaid",
  "icons": {
    "16": "icons/default.png",
    "48": "icons/default.png",
    "128": "icons/default.png"
  },
  "permissions": [
    "tabs",
    "webRequest",
    "webNavigation",
    "webRequestBlocking",
    "storage",
    "management",
    "http://*/*",
    "https://*/*"
  ],
  "default_locale": "en",
  "background": {
    "page": "src/background/index.html",
    "persistent": true
  },
  "options_page": "src/options/index.html",
  "browser_action": {
    "default_icon": "icons/default.png",
    "default_title": "BlockAid",
    "default_popup": "src/popup/index.html"
  }
}